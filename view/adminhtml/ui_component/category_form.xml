<?xml version="1.0" encoding="UTF-8"?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="content">
        <field name="cms_page" sortOrder="60" formElement="select">
            <settings>
                <dataType>string</dataType>
                <label translate="true">Add CMS Page</label>
                <tooltip>
                    <description>Use to add a CMS Page to the navigation tree. This requires the Display Mode to be set to "CMS Page".</description>
                </tooltip>
            </settings>
        </field>
    </fieldset>

    <fieldset name="search_engine_optimization">
        <field name="custom_url" sortOrder="60" formElement="input">
            <settings>
                <dataType>string</dataType>
                <label translate="true">Custom URL</label>
                <tooltip>
                    <description>Use to add a Custom URL to the navigation tree. This requires the Display Mode to be set to "Custom URL".</description>
                </tooltip>
            </settings>
        </field>
    </fieldset>
</form>
